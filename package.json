{
  "name": "@sserov/eslint-plugin-dirnames",
  "version": "1.0.0",
  "description": "Eslint rule for consistent dirnames",
  "author": "Aleksandr Serov",
  "bugs": {
    "url": "https://github.com/AleksandrSerov/eslint-plugin-dirnames/issues"
  },
  "license": "ISC",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AleksandrSerov/eslint-plugin-dirnames.git"
  },
  "scripts": {
    "test": "jest .",
    "coverage": "jest . --coverage",
    "release-patch": "npm version patch -m 'chore(*): patch version'",
    "release-minor": "npm version minor -m 'chore(*): minor version'",
    "release-major": "npm version major -m 'chore(*): major version'",
    "postversion": "git push origin master && git push --tags && npm run pub",
    "pub": "npm publish --access public"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "peerDependencies": {
    "kebab-case": "1.0.0"
  },
  "dependencies": {
    "kebab-case": "1.0.0"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "jest": "^25.2.7"
  },
  "homepage": "https://github.com/alfa-laboratory/eslint-plugin-dirnames#readme",
  "directories": {
    "lib": "lib",
    "test": "tests"
  },
  "keywords": [
    "dirnames",
    "kebab",
    "kebab-case",
    "case",
    "format",
    "filenames"
  ]
}
